{
  "name": "toukey",
  "version": "1.0.0",
  "description": "Toukey is a Javascript library for keyboard shortcuts",
  "main": "index.js",
  "scripts": {
    "demo": "npx rollup -c rollup.config.demo.js -w",
    "build:dev": "npx rollup -c rollup.config.dev.js",
    "build:prod": "npx rollup -c rollup.config.prod.js",
    "prettier": "npx prettier * --write",
    "lint": "npx eslint src/* --fix",
    "test": "jest",
    "publish": "npm run build:dev && npm run build:prod && npm publish"
  },
  "keywords": [
    "keyboard",
    "keyboardevent",
    "shortcut",
    "hotkey",
    "event",
    "keydown",
    "keyup",
    "library "
  ],
  "files": [
    "dist"
  ],
  "author": "Yukiniro",
  "license": "MIT",
  "homepage": "https://github.com/Yukiniro/toukey",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukiniro/toukey.git"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "rollup": "^2.67.3",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
